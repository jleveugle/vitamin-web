{
  "name": "@decathlon/vitamin-wc-button",
  "private": true,
  "version": "0.0.1",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/vitamin-wc-button/vitamin-wc-button.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "dev": "chokidar \"../../../../../node_modules/@decathlon/vitamin-css-button/dist/*.css\" \"src/**\" -c \"stencil build\"",
    "build:dev": "lerna exec --scope='@decathlon/vitamin-wc-button' --include-dependencies -- npm run build --if-present",
    "start:dev": "yarn build:dev && lerna exec --parallel --scope='@decathlon/vitamin-wc-button' --include-dependencies -- npm run dev --if-present",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1"
  },
  "devDependencies": {
    "@decathlon/vitamin-css-button": "^0.0.1",
    "@stencil/postcss": "^2.0.0",
    "@stencil/react-output-target": "^0.0.9",
    "@stencil/svelte-output-target": "^0.0.3",
    "svelte": "^3.31.2",
    "chokidar-cli": "^2.1.0"
  },
  "license": "MIT"
}
